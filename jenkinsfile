pipeline {
    agent {label 'linux'}
    stages {
        stage('build'){
            steps {
                echo "Etapa build no disponible"
            }
        }
        stage('test'){
            steps {
                echo "Etapa test no disponible"
            }
        }
        stage('deploy'){
            steps {
                sh "git clone https://github.com/cristianmorac/jenkins-practice.git"
            }
        }
    }

}